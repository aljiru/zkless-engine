.box{color:#fe33ac;border-color:#fdcdea}.box div{-webkit-box-shadow:0 0 5px rgba(0,0,0,0.3);box-shadow:0 0 5px rgba(0,0,0,0.3)}.myDiv{background:${c:encodeURL('~./path/to/image.png')}}.myDiv{background:${c:encodeThemeURL('~./path/to/image.png')}}.myDiv{background:rgba(200,200,200,0.8);<c:if test="${zk.ie <= 8}">background:#c8c8c8;</c:if>}.myDiv{${t:applyCSS3('transition-property','opacity')};${t:applyCSS3('transition-duration','350ms')}}.myDiv{${t:transform('translate3d(0, 0, 0)')}}.myDiv{${t:boxShadow('1px 1px 0 rgba(0, 0, 0, 0.2)')}}.myDiv1{${t:box('box-flex','1')}}.myDiv2{${t:box2('box-flex','1','box-orient','horizontal')}}.myDiv3{${t:box3('box-flex','1','box-orient','horizontal','box-pack','center')}}.myDiv{${t:gradient('ver','#FFFFFF 0%; #EEEEEE 100%')}}.z-component1{background-image:url(${c:encodeURL('~./path/to/image1.png')})}.z-component2{background-image:url(${c:encodeThemeURL('~./path/to/image2.png')})}.z-component3{background:url(${c:encodeThemeURL('~./path/to/image3.png')}) center center}.z-component1{${t:gradient('ver','#333333 0%; #555555 50%; #666666 100%')}}.z-component2{${t:gradient('hor','#333333 0%; #666666 100%')}}.z-component3{${t:gradient('ver','#333333 0%; #666666 100%')}}.ie8 .z-component4{background:#4d4d4d}.z-component1:hover{background:0;filter:progid:DXImageTransform.Microsoft.gradient(enabled=false);background:#777}.z-component1{${t:borderRadius('3px')}}.z-component2{${t:borderRadius('3px 3px 0 0')}}.z-component3{${t:borderRadius('0 3px 3px 0')}}.z-component4{${t:borderRadius('0 0 3px 3px')}}.z-component5{${t:borderRadius('3px 0 0 3px')}}.z-component{${t:boxShadow('inset 1px 1px 0 #222222')}}